<Window x:Class="Newproject.ViewUsageData"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Newproject"
        mc:Ignorable="d"
        Title="ViewUsageData" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="studentNameBox" HorizontalContentAlignment="Center" Height="43" Margin="314,172,286,205" TextWrapping="Wrap" Text="Enter student name..." VerticalContentAlignment="Center" Width="192" Background="Red" Foreground="White"/>
        <Button x:Name="submit2" Content="Submit" HorizontalAlignment="Left" Height="30" Margin="366,220,0,0" VerticalAlignment="Top" Width="85" Background="Red" Foreground="White" Click="submit2_Click"/>
        <Button x:Name="back" Content="" HorizontalAlignment="Left" Height="55" Margin="695,10,0,0" VerticalAlignment="Top" Width="87" BorderBrush="White" Click="back_Click">
            <Button.Background>
                <ImageBrush ImageSource="back.png"/>
            </Button.Background>
        </Button>
    </Grid>
</Window>
